{
  "name": "json-to-model-format",
  "description": "Json to model format",
  "version": "1.1.14",
  "private": false,
  "author": "imagine10255@gmail.com",
  "homepage": "http://imagine10255.github.io/json-to-model-format",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imagine10255/json-to-model-format"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=14.5.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build PUBLIC_URL=/json-to-model-format",
    "test": "react-scripts test",
    "gitlog": "git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cblueby %an %Cgreen(%cr)%Creset'",
    "lint:fix": "eslint ./src --fix",
    "cz": "git cz",
    "release": "standard-version --release-as",
    "release:major": "standard-version -r major",
    "release:minor": "standard-version -r minor",
    "release:patch": "standard-version -r patch",
    "release:beta": "standard-version --prerelease beta",
    "release:alpha": "standard-version --prerelease alpha",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@types/classnames": "2.3.1",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.169",
    "@types/node": "15.0.3",
    "@types/qs": "6.9.6",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.5",
    "@types/react-html-parser": "2.0.1",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.9",
    "@types/webpack-env": "1.16.0",
    "bowser": "2.11.0",
    "classnames": "2.3.1",
    "lodash": "4.17.21",
    "path-to-regexp": "6.2.0",
    "qs": "6.9.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.8.3",
    "react-html-parser": "2.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "5.3.0",
    "typescript": "4.1.5",
    "universal-cookie": "4.0.4",
    "web-vitals": "1.1.2",
    "workbox-background-sync": "5.1.3",
    "workbox-broadcast-update": "5.1.3",
    "workbox-cacheable-response": "5.1.3",
    "workbox-core": "5.1.3",
    "workbox-expiration": "5.1.3",
    "workbox-google-analytics": "5.1.3",
    "workbox-navigation-preload": "5.1.3",
    "workbox-precaching": "5.1.3",
    "workbox-range-requests": "5.1.3",
    "workbox-routing": "5.1.3",
    "workbox-strategies": "5.1.3",
    "workbox-streams": "5.1.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "enquirer": "2.3.6",
    "gh-pages": "^3.2.3",
    "http-proxy-middleware": "2.0.0",
    "ora": "5.4.0",
    "standard-version": "9.3.0",
    "ts-node": "9.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "quotes": [
        "warn",
        "single"
      ],
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "warn",
        4
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "import/no-anonymous-default-export": "off"
    }
  }
}
